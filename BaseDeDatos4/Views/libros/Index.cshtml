@model List<BaseDeDatos4.Models.ViwModels.ListTablaViwModels>
@{
    ViewBag.Title = "libros";
}

<h2>@ViewBag.title</h2>

<div class="row">
    <div class="col-lg-12">
        <table>
            <tr>
                <th>#</th>
                <th>nombre</th>
                <th>precio venta</th>
            </tr>
            @foreach (var oElemento in Model)
            {
                <tr>
                    <th>@oElemento.id_libro</th>
                    <th>@oElemento.nombre</th>
                    <th>@oElemento.precio_venta</th>
                </tr>
            }

        </table>

    </div>


</div>